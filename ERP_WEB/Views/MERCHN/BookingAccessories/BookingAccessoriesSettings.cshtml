@{
    ViewBag.Title = "Booking Accessories";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style type="text/css">
    .row {
        margin-bottom: 5px;
    }

    .k-grid-footer {
        display: none;
    }

    /*.k-grid .k-grid-search {
        margin-left: auto;
        margin-right: 0;
    }*/
</style>
@{ string jsVer = @System.Configuration.ConfigurationManager.AppSettings["JSVer"];}
@Scripts.Render("~/Scripts/MERCHN/BookingAccessories/BookingAccessoriesSettings.js?" + @jsVer)

<div class="row">
    <div id="divBookingAccessoriesSummary" class="col-lg-12 ">
        @Html.Partial("../MERCHN/BookingAccessories/_BookingAccessoriesSummary")
    </div>
    <div id="divBookingAccessoriesDetails" class="col-lg-12 displayNone">
        @Html.Partial("../MERCHN/BookingAccessories/_BookingAccessoriesDetails")
    </div>
</div>


