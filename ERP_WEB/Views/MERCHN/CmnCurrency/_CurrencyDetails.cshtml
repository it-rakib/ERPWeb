@Scripts.Render("~/Scripts/MERCHN/Currency/CurrencyDetails.js")
<div class="bs-example">
    <div class="well well-sm">Currency Details</div>
    <div id="menuDetailsDiv" class="row">
        <div class="col-md-12 col-xs-12">
            <div class="form-horizontal">
                <div class="form-group">
                    <input type="hidden" id="hdnCurrencyId" value="00000000-0000-0000-0000-000000000000" />
                    <label class="col-md-4 col-xs-4 control-label" for="txtColorName">Currency Name:</label>
                    <div class="col-md-6 col-xs-6">
                        <input type="text" placeholder="Currency Name" name="txtCurrencyName" id="txtCurrencyName" class="form-control" style="width: 100%; height:28px;" required validationmessage="*" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-4 col-xs-4 control-label" for="txtCurrencyCode">Currency Code:</label>
                    <div class="col-md-6 col-xs-6">
                        <input type="text" placeholder="Currency Code" name="txtCurrencyCode" id="txtCurrencyCode" class="form-control" style="width: 100%; height:28px;" required validationmessage="*" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-4 col-xs-4 control-label" for="txtCurrenceySymbol">Currencey Symbol:</label>
                    <div class="col-md-6 col-xs-6">
                        <input type="text" placeholder="Currencey Symbol" name="txtCurrenceySymbol" id="txtCurrenceySymbol" class="form-control" style="width: 100%; height:28px;" required validationmessage="*" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-4 col-xs-4 control-label" for="numOrderSl">Order Sl:</label>
                    <div class="col-md-6 col-xs-6">
                        <input type="number" placeholder="Order Sl" name="numOrderSl" id="numOrderSl" class="form-control" style="width: 100%; height:28px;" required validationmessage="*" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 col-xs-12">
                        <ul class="list-group ">
                            <li class="list-group-item centerAlign">
                                <button id="btnSave" class="btn btn-primary" type="button">Save</button>
                                <button id="btnClearAll" class="btn btn-primary">Clear</button>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
