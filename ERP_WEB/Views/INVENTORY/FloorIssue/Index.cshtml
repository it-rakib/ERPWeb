    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Scripts.Render("~/Scripts/INVENTORY/FloorIssue/FloorIssueSettings.js")

<div class="row">
    <div id="divFloorIssueSummary" class="col-lg-12">
        @Html.Partial("../INVENTORY/FloorIssue/_FloorIssueSummary")
    </div>
    <div id="divFloorIssueDetails" class="col-lg-12 displayNone">
        @Html.Partial("../INVENTORY/FloorIssue/_FloorIssueDetails")
    </div>
