@Scripts.Render("~/Scripts/INVENTORY/InventoryQC/InvQCDetails.js")
<div class="row">
    <div class="col-md-12 col-xs-12">
        <div class="bs-example">
            <div class="well well-sm">INVENTORY QUALITY CHECK (QC)</div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="col-md-7"></div>
                        <div class="col-md-5">
                            <label class="control-label" style="line-height: 20px; color: green; font-weight: bold; font-size: 18px !important; float: left;" id="lblMessage"></label>
                            <button id="btnSaveQC" class="k-button-success" style="float: right;"><i class="fa fa-save" style="margin-right: 4px;"></i>Save</button>
                            <button id="btnBackQCList" class="k-button-danger" style="float: right;"><i class="fa fa-backward" style="margin-right: 4px;"></i>Back</button>
                        </div>
                    </div>
                </div>
                <hr/>
                <div class="row">
                    <div class="col-md-12">
                        <div class="col-md-3">
                            <div class="form-horizontal">
                                <input type="hidden" id="hdnQCId" value="00000000-0000-0000-0000-000000000000" />
                                <input type="hidden" id="hdnMrrId" value="00000000-0000-0000-0000-000000000000" />                             
                                <div class="form-group required">
                                    <label class="col-md-4 col-xs-4 control-label" for="txtQCNo">QC No:</label>
                                    <div class="col-md-7 col-xs-7 ">
                                        <input type="text" class="form-control" style="width: 100%; height: 28px;" name="txtQCNo" id="txtQCNo" disabled="disabled" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-horizontal">
                                <div class="form-group required">
                                    <label class="col-md-4 col-xs-4 control-label" for="dtQCDate">QC Date:</label>
                                    <div class="col-md-7 col-xs-7 ">
                                        <input type="text" class="datepicker" style="width: 100%; height: 28px;" name="dtQCDate" id="dtQCDate" required validationmessage="Required" disabled/>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-horizontal">
                                <div class="form-group required">
                                    <label class="col-md-4 col-xs-4 control-label" id="lblSearchPI" for="txtPINo">PI. NO:</label>
                                    <div class="col-md-7 col-xs-7">
                                        <div class="input-group add-on">
                                            <input type="hidden" id="hdnPIId" value="00000000-0000-0000-0000-000000000000" />  
                                            <input type="text" class="form-control" style="width: 100%; height: 28px;" name="txtPINo" id="txtPINo" disabled="disabled" />
                                            <div class="input-group-btn">
                                                <button class="btn btn-xs btn-info" style="height:28px;" type="button" id="btnSearchPIInfo"><i class="glyphicon glyphicon-search"></i></button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-horizontal">
                                <div class="form-group required">
                                    <label class="col-md-4 col-xs-4 control-label" id="lblSearchConsignment" for="txtConsignmentNo">Consignment NO:</label>
                                    <div class="col-md-7 col-xs-7">
                                        <div class="input-group add-on">
                                            <input type="hidden" id="hdCPIId" value="00000000-0000-0000-0000-000000000000" />  
                                            <input type="hidden" id="hdnConsignmentId" value="00000000-0000-0000-0000-000000000000" />
                                            <input type="text" class="form-control" style="width: 100%; height: 28px;" name="txtConsignmentNo" id="txtConsignmentNo" disabled="disabled" />
                                            <div class="input-group-btn">
                                                <button class="btn btn-xs btn-info" style="height:28px;" type="button" id="btnSearchConsignmentInfo"><i class="glyphicon glyphicon-search"></i></button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        @Html.Partial("../Inventory/InventoryQC/_ReceivedItemList")
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

