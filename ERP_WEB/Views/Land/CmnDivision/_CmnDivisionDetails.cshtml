@Scripts.Render("~/Scripts/Land/CmnDivision/CmnDivisionDetails.js")
<div class="bs-example">
    <div class="well well-sm">Division Information</div>
    <div id="menuDetailsDiv" class="row">
        <div class="col-md-12 col-xs-12">
            <div class="form-horizontal">
                <div class="form-group">
                    <input type="hidden" id="hdnCmnDivisionId" value="00000000-0000-0000-0000-000000000000" />
                    <label class="col-md-4 col-xs-4 control-label" for="txtCmnDivisionName">Division Name:</label>
                    <div class="col-md-6 col-xs-6">
                        <input type="text" placeholder="Division Name..." name="txtCmnDivisionName" id="txtCmnDivisionName" class="form-control" required validationmessage="*" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 col-xs-12">
                        <ul class="list-group ">
                            <li class="list-group-item centerAlign">
                                <button id="btnSave" class="btn btn-primary" type="button">Save</button>
                                <button id="btnClearAll" class="btn btn-primary">Clear</button>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@*Test*@