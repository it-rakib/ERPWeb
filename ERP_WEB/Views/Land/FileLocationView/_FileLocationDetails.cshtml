@Scripts.Render("~/Scripts/Land/FileLocationView/FileLocationDetails.js")
<div class="bs-example">
    <div class="well well-sm">File Location Details</div>
    <div class="row" style="margin-bottom:0px !important;">
        <div class="col-md-12 col-xs-12 centerAlign">
            <ul class="list-group" style="margin-bottom:2px !important;">
                <li class="list-group-item rightAlign">
                    <button id="btnCancelApplication" class="k-button-danger" type="button"><span class="glyphicon glyphicon-circle-arrow-left"></span>Back</button>
                </li>
            </ul>
        </div>
    </div>
    <input type="hidden" id="hdnFileLocationMasterId" value="00000000-0000-0000-0000-000000000000" />
    <div class="row">
        <div class="col-md-3 col-xs-3">
            <div class="form-group">
                <label class="control-label" style="margin-top : 4px" for="cmbFileCode">File Code</label>
                <div class="comboBox">
                    <input id="cmbFileCode" placeholder="--Select File Code--" style="width: 80%; height:28px;" required validationmessage="*" disabled="disabled"/>
                </div>
            </div>
        </div>

        <div class="col-md-3 col-xs-3">
            <div class="form-group">
                <label class="control-label" style="margin-top : 4px" for="cmbFileNo">File No</label>
                <div class="comboBox">
                    <input id="cmbFileNo" placeholder="--Select File No--" style="width: 80%; height:28px;" required validationmessage="*" disabled="disabled"/>
                </div>
            </div>
        </div>

        <div class="col-md-3 col-xs-3">
            <div class="form-group">
                <label class="control-label" style="margin-top : 4px" for="cmbAlmirahNo">Almirah/Shelf No</label>
                <div class="comboBox">
                    <input id="cmbAlmirahNo" placeholder="--Select Almirah/Shelf No--" style="width: 80%; height:28px;" required validationmessage="*" disabled="disabled"/>
                </div>
            </div>
        </div>

        <div class="col-md-3 col-xs-3">
            <div class="form-group">
                <label class="control-label" style="margin-top : 4px" for="cmbRackNo">Rack No</label>
                <div class="comboBox">
                    <input id="cmbRackNo" placeholder="--Select Rack No--" style="width: 80%; height:28px;" required validationmessage="*" disabled="disabled"/>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-6 col-xs-6">
            <div class="form-group">
                <label class="control-label" style="margin-top : 4px" for="cmbDeedNo">Deed No</label>
                <div class="comboBox">
                    <input id="cmbDeedNo" placeholder="--Select Deed No--" style="width: 100%; height:28px;" required validationmessage="*" disabled="disabled"/>
                </div>
            </div>
        </div>

        @*<div class="col-md-2 col-xs-2" style="margin-top: 25px;">
            <button id="btnAddDeed" class="k-button-primary"><i class="fas fa-arrow-down"></i>Add Deed</button>
        </div>*@
    </div>

    <div class="row">
        <div class="col-md-6 col-xs-6">
            <div class="bs-example">
                <div class="panel-body">
                    <div id="grdFileLocationDetail"></div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-4 col-xs-4">
            <div class="form-group">
                <label class="control-label" style="margin-top: 4px" for="txtRemarks">Remarks</label>
                <div class="">
                    <textarea placeholder="Remarks" name="txtRemarks" id="txtRemarks" style="width: 100%; height: 70px; resize: none;" required validationmessage="*" class="form-control" disabled="disabled"></textarea>
                </div>
            </div>
        </div>
    </div>
</div>