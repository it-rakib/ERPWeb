
@Scripts.Render("~/Scripts/TRANSPORT/VehicleBrand/VehicleBrandDetails.js")
<div class="bs-example">
    <div class="well well-sm">Brand Details</div>
    <div id="menuDetailsDiv" class="row">
        <div class="col-md-12 col-xs-12">
            <div class="form-horizontal">
                <div class="row">
                    <div class="form-group">
                        <input type="hidden" id="hdnBrandId" value="0" />
                        <label class="col-md-4 col-xs-4 control-label" for="txtBrandName" style="margin-top : 5px;">Brand Name:</label>
                        <div class="col-md-6 col-xs-6">
                            <input type="text" placeholder="Type Brand Name.." name="txtBrandName" id="txtBrandName" class="form-control" style="margin-top:2px;" required validationmessage="*" />
                        </div>
                    </div>
                    <div class="form-group displayNone">
                        <label class="col-md-4 col-xs-4 control-label" for="chkIsDeleted" style="margin-bottom : 2px;">Is Delete :</label>
                        <div class="col-md-6 col-xs-6">
                            <input type="checkbox" id="chkIsDeleted">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 col-xs-12">
                        <ul class="list-group ">
                            <li class="list-group-item centerAlign">
                                <button id="btnSave" class="btn btn-primary" type="button">Save</button>
                                <button id="btnClearAll" class="btn btn-primary">Clear</button>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

