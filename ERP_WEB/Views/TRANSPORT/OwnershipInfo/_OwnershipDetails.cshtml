
@Scripts.Render("~/Scripts/TRANSPORT/OwnershipInfo/OwnershipDetails.js")
<div class="bs-example">
    <div class="well well-sm">OwnerShip Details</div>
    <div id="menuDetailsDiv" class="row">
        <div class="col-md-12 col-xs-12">
            <div class="form-horizontal">
                <div class="row">
                    <div class="form-group">
                        <input type="hidden" id="hdnOwnershipId" value="0" />
                        <label class="col-md-4 col-xs-4 control-label" for="txtOwnershipName" style="margin-top : 5px;">OwnerShip Name:</label>
                        <div class="col-md-6 col-xs-6">
                            <input type="text" placeholder="OwnerShip Name" name="txtOwnershipName" id="txtOwnershipName" class="form-control" style="margin-top:2px;" required validationmessage="*" />
                        </div>
                    </div>
                    <div class="form-group displayNone">
                        <label class="col-md-4 col-xs-4 control-label" for="chkIsDeleted" style="margin-bottom : 2px;">Is Delete :</label>
                        <div class="col-md-6 col-xs-6">
                            <input type="checkbox" id="chkIsDeleted">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 col-xs-12">
                        <ul class="list-group ">
                            <li class="list-group-item centerAlign">
                                <button id="btnSave" class="btn btn-primary" type="button">Save</button>
                                <button id="btnClearAll" class="btn btn-primary">Clear</button>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
