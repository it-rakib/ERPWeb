                                <input type="text" placeholder="Engine No" name="txtEngineNo" id="txtEngineNo" style="width: 60%; height: 28px;" class="form-control" />
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="cmbVehicleCondition" style="margin-bottom: 6px;">Vehicle Condition:</label>
                            <div>
                                <input type="text" placeholder="Vehicle Condition" name="cmbVehicleCondition" id="cmbVehicleCondition" style="width: 60%; height: 28px;"  />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label" for="cmbVehicleBrand" style="margin-bottom: 6px;">Brand:</label>
                            <div>
                                <input type="text" placeholder="Vehicle Brand" name="cmbVehicleBrand" id="cmbVehicleBrand" style="width: 60%; height: 28px;"  />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label" for="cmbVehicleModel" style="margin-bottom: 6px;">Model:</label>
                            <div>
                                <input type="text" placeholder="Vehicle Model" name="cmbVehicleModel" id="cmbVehicleModel" style="width: 60%; height: 28px;"  />
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="control-label" style="margin-bottom: 6px;" for="txtRentalCompany">Rental Company:</label>
                            <div>
                                <input type="text" placeholder="Rental Company" name="txtRentalCompany" id="txtRentalCompany" style="width: 60%; height: 28px;" class="form-control" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label" for="txtRentalContact">Rental Contact:</label>
                            <div>
                                <input type="text" placeholder="Rental Contact" name="txtRentalContact" id="txtRentalContact" style="width: 60%; height: 28px;" class="form-control" />
