                            <label class="control-label" style="margin-bottom: 6px;" for="cmbPrimaryFuelType">Primary Fuel Type:</label>
                            <div class="comboBox">
                                <input id="cmbPrimaryFuelType" placeholder="--Select Primary Fuel Type--" style="width: 60%; height: 28px;"  />
