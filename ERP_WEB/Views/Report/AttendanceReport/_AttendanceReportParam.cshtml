<div class="col-md-12 col-xs-12 displayNone" id="divMonthlyAtt">
    <div class="col-md-12 col-xs-12 bs-example">
        <div class="well well-sm">
            Monthly Attendance Report
        </div>
        <div class="col-md-12">
            <div class="row">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-md-3  control-label" for="txtMonthYear" id="lblMonthYearFrom">Select Month:</label>
                        <div class="col-md-9 ">
                            <input type="text" name="txtMonthYear" style="width: 100%; height: 28px;" id="txtMonthYear" required validationmessage="Required">
                        </div>
                    </div>
                    <div class="form-group displayNone" id="divMonthTo">
                        <label class="col-md-3  control-label" for="txtMonthYearTo">Month To:</label>
                        <div class="col-md-9 ">
                            <input type="text" name="txtMonthYearTo" style="width: 100%; height: 28px;" id="txtMonthYearTo" required validationmessage="Required">
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="spacer5"></div>
                <ul class="list-group ">
                    <li class="list-group-item centerAlign">
                        <button id="btnMonthlyAttSummaryReport" class="k-button-linkbutton" type="button">Employee Attendance Summary</button>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

