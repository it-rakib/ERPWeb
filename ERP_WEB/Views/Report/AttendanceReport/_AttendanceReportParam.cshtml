<div class="col-md-12 col-xs-12">
    <div class="col-md-12 col-xs-12 bs-example">
        <div class="col-md-12">
            <div class="row">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-md-3  control-label" for="txtMonthYear">Select Report:</label>
                        <div class="col-md-9 ">
                            <input type="text" name="cmbReportName" style="width: 100%; height: 28px;" id="cmbReportName" required validationmessage="Required">
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
<div class="col-md-12 col-xs-12 displayNone" id="divJobLocation">
