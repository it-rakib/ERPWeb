<script src="~/Scripts/HRM/Reports/MonthlyAttReport2/ManualOfficialInOutUpdate.js"></script>
<div class="row">
    <div class="col-md-12 col-xs-12">
        @*<div class="well well-sm">Manual Official In/Out Application</div>*@
        <div class="bs-example" id="divOfficialInOutRegDetails">
            <div class="form-horizontal">
                <input type="hidden" id="hdnOfficialInOutId" value="0" />
                <input type="hidden" id="hdnOfficialInOutOwnerId" value="0" />
                <div class="form-group">
                    <label class="col-md-3 col-xs-3 control-label" for="txtInOutDate">In/Out Date:</label>
                    <div class="col-md-9 col-xs-9">
                        <input type="text" style="width: 100%; height: 28px;" name="txtInOutDate" id="txtInOutDate" required validationmessage="Required" disabled="disabled" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 col-xs-3 control-label" for="txtTimeFrom">Time From:</label>
                    <div class="col-md-3 col-xs-3">
                        <input type="text" style="width: 100%; height: 28px;" name="txtTimeFrom" id="txtTimeFrom" required validationmessage="Required" />
                    </div>
                    <label class="col-md-3 col-xs-3 control-label" for="txtTimeTo">Time To:</label>
                    <div class="col-md-3 col-xs-3">
                        <input type="text" style="width: 100%; height: 28px;" name="txtTimeTo" id="txtTimeTo" required validationmessage="Required" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-3 col-xs-3 control-label" for="txtNoOfDaysIO">Days:</label>

                    <div class="col-md-9 col-xs-9">
                        <input type="text" class="form-control" style="height: 28px;" name="txtNoOfDaysIO" id="txtNoOfDaysIO" required validationmessage="Required" readonly="readonly" />
                    </div>

                </div>
                <div class="form-group">
                    <label class="col-md-3 col-xs-3 control-label" for="cmbInOutType">Type:</label>
                    <div class="col-md-9 col-xs-9">
                        <input type="text" style="width: 100%; height: 28px;" name="cmbInOutType" id="cmbInOutType" required validationmessage="Required" />
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-md-3 col-xs-3 control-label" for="txtReason">Reason:</label>
                    <div class="col-md-9 col-xs-9">
                        <textarea type="text" class="form-control" style="width: 100%; height: 100px; resize: none" name="txtReason" id="txtReason"></textarea>
                    </div>
                </div>
            </div>
            <div class="col-md-3 col-xs-3"></div>
            <div class="row">
                <div class="spacer5"></div>
                <div class="col-md-12 col-xs-12">
                    <ul class="list-group ">
                        <li class="list-group-item centerAlign">
                            <button id="btnSaveOfficialInOut" class="k-button-success" type="button"><span class="glyphicon glyphicon-floppy-disk"></span> Apply</button>
                            <button id="btnClearIO" class="k-button-danger" type="button"><span class="glyphicon glyphicon-repeat"></span> Close</button>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
